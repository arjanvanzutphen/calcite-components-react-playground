{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "printWidth": 100
      }
    },
    {
      "files": "*.ts",
      "options": {
        "printWidth": 100
      }
    },
    {
      "files": "*.js",
      "options": {
        "printWidth": 100
      }
    }
  ]
}
